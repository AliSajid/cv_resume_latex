    author = {Lin, Boren and Jin, Zhen and Park, Gyeongah and Ge, Qian and Singh, Kritika and Ryan V, William G. and Imami, Ali Sajid and Naghavi, Farzaneh and Miller, Olivia Ann and Khan, Saira and Lu, Hui and McCullumsmith, Robert E. and Du, Jianyang},
    month = oct,
